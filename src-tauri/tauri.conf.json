{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Voose",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true,
        "scope": ["$PUBLIC/*"]
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.voose.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "json/DRIVER.json",
        "json/BOIS.json",
        "json/HYBRIDE.json",
        "json/UFER.json",
        "json/SFER.json",
        "json/PUTTER.json",
        "json/HTEXTILE.json",
        "json/BTEXTILE.json",
        "json/optionannee.json",
        "json/optioncategorie.json",
        "json/optionfamily.json",
        "json/optionmarque.json",
        "json/optionsexe.json",
        "json/optiontypeproduct.json"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Voose",
        "width": 1400,
        "height": 800
      }
    ]
  }
}
